{
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "none",
  "proseWrap": "never",
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "vueIndentScriptAndStyle": true,
  "htmlWhitespaceSensitivity": "strict",
  "insertPragma": false,
  "requirePragma": false,
  "endOfLine": "lf",
  "rangeStart": 0
}
